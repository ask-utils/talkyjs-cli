  "homepage": "https://github.com/ask-utils/talkyjs-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "talky",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "ask-utils/talkyjs-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
