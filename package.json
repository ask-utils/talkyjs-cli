{
  "name": "@talkyjs/cli",
  "version": "1.1.1",
  "author": "Hidetaka Okamoto",
  "bin": {
    "talky": "./bin/run",
    "talkyjs": "./bin/run"
  },
  "bugs": "https://github.com/ask-utils/talkyjs-cli/issues",
  "dependencies": {
    "@angular-devkit/schematics-cli": "^0.901.9",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@talkyjs/schematics": "2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
